                            <tbody>  
                                <tr class="col">
                                    <td class="col-6"><div>
                                <label class=" col-form-label">Ingrese número de nómina del empleado</label>
                            <input type="number" class="form-control  col-form-label" name="ReInciEmpId" id="ReInciEmpId"  onchange="ValidarEmpleado()">
                        </div></td>
                                    <td class="col-6"><div >
                                <label class=" col-form-label">Nombre del empleado</label>
                            <input type="text" class="form-control  col-form-label" name="ReInciEmpNombreCompleto" id="ReInciEmpNombreCompleto" readonly>
                            </div></td>
                                </tr>
                                <tr>
                                    <td><div >
                                <label class=" col-form-label">¿Tiene horario permanente?</label>
                            <input type="text" class="form-control  col-form-label" name="HorarioPermanente" id="HorarioPermanente" readonly>
                        </div></td>
                                    <td><div >
                                <label class=" col-form-label">¿Está activo el empleado?</label>
                            <input type="text" class="form-control  col-form-label" name="Estatus" id="Estatus" readonly>
                        </div></td>
                                </tr>
                                <tr>
                                    <td><div >
                                <label class=" col-form-label">Fecha</label>
                            <input type="date" class="form-control  col-form-label" name="Fecha" id="Fecha" readonly>
                        </div></td>
                                    <td><div >
                                <label class=" col-form-label">Ingrese Tipo de Permiso</label>
                            <select class="form-control col-form-label" asp-items="ViewBag.Inci" id="ReInciInciId" name="ReInciInciId" onchange="SeleccionarIncidencia()" required>
                                <option selected disabled value="">Seleccione una opción</option>
                            </select>
                        </div></td>
                                </tr>
                        </tbody>
                                </table>
                       

                        <!-- ************** FORMULARIO TABLA DISEÑO DE INCIDENCIA **************  -->
