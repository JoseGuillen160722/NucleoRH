                    <th scope="col">Empleado</th>
                    <th scope="col">Fecha</th>
                    <th scope="col">Tipo de Incidencia</th>
                    <th scope="col">Estado</th>
                    <th>Acciones</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var ins in ViewBag.RegistroIncidenciasPropias)
               {
                   <tr>
                       <td>@ins.ReInciId</td>
                       <td>@ins.Emp.EmpNombreCompleto</td>
                       <td>@ins.Fecha.ToShortDateString()</td>
                       <td>@ins.Inci.InciDescripcion</td>
                       <td>@ins.Estatus.EstDescripcion</td>
